{
  "name": "yogstation-ui",
  "version": "0.3.13",
  "description": "Yogstation Website UI (Beta)",
  "main": "lib/index.js",
  "scripts": {
    "test:at": "cypress",
    "test:unit": "mocha --require @babel/register --recursive",
    "test": "run-p test:at test:unit",
    "start": "run-p start:client start:server",
    "start:client": "webpack-dev-server --config ./webpack.dev.js",
    "start:server": "node ./scripts/localServerRunner.js",
    "build:client": "webpack --config ./webpack.prod.js",
    "build:server": "webpack --config ./webpack.server.js",
    "git:amend": "git commit --amend --no-edit",
    "terraform:refresh": "node ./scripts/refresh_terraform_outputs.js",
    "terraform:apply": "node ./scripts/update_version_terraform.js",
    "build": "run-p build:client build:server",
    "deploy:client": "node scripts/deploy_client",
    "deploy:server": "node scripts/deploy_server",
    "deploy": "run-p deploy:client deploy:server",
    "verify:server": "run-s test:unit build:server",
    "verify:client": "run-s test:unit build:client",
    "verify": "run-p verify:server verify:client",
    "version:patch": "npm version patch",
    "complete": "run-s terraform:refresh verify version:patch build deploy terraform:apply git:amend"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/BASIC-BIT/yogstation-ui.git"
  },
  "keywords": [
    "yogstation"
  ],
  "author": "BASIC-BIT",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BASIC-BIT/yogstation-ui/issues"
  },
  "homepage": "https://github.com/BASIC-BIT/yogstation-ui#readme",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "antd": "^3.13.2",
    "aws-sdk": "^2.398.0",
    "chardet": "^0.7.0",
    "connected-react-router": "^6.3.1",
    "express": "^4.16.4",
    "fuse.js": "^3.4.2",
    "history": "^4.7.2",
    "jsonwebtoken": "^8.5.1",
    "jszip": "^3.1.5",
    "less": "^3.9.0",
    "mysql": "^2.16.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-html-parser": "^2.0.2",
    "react-redux": "^6.0.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "shelljs": "^0.8.3",
    "striptags": "^3.1.1",
    "xhr": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-inline-import": "^3.0.0",
    "chai": "^4.2.0",
    "chance": "^1.0.18",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "depcheck": "^0.7.2",
    "less-loader": "^4.1.0",
    "less-vars-to-js": "^1.3.0",
    "mime-types": "^2.1.21",
    "mocha": "^5.2.0",
    "node-sass": "^4.11.0",
    "proxyquire": "^2.1.0",
    "react-hot-loader": "^4.6.5",
    "sass-loader": "^7.1.0",
    "sinon": "^7.2.3",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.1",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-encoding-plugin": "^0.3.1",
    "webpack-merge": "^4.2.1"
  }
}
